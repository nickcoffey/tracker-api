{
  "name": "tracker-api",
  "version": "1.0.0",
  "description": "Workout tracker graphql api",
  "main": "dist/index.js",
  "scripts": {
    "start": "node dist/server",
    "clean": "rm -rf dist",
    "build": "yarn -s run clean && yarn -s run generate && tsc -p .",
    "generate": "yarn -s run generate:prisma && yarn -s run generate:nexus",
    "generate:prisma": "prisma introspect && prisma generate",
    "generate:nexus": "ts-node --transpile-only src/schema",
    "dev": "ts-node-dev --no-notify --respawn --transpileOnly src/server"
  },
  "author": "Nick Coffey",
  "license": "MIT",
  "dependencies": {
    "@nexus/schema": "^0.16.0",
    "@prisma/cli": "^2.9.0",
    "@prisma/client": "^2.9.0",
    "@types/node-fetch": "^2.5.7",
    "apollo-server": "^2.11.0",
    "graphql": "^15.3.0",
    "log4js": "^6.3.0",
    "nexus-plugin-prisma": "^0.22.0"
  },
  "devDependencies": {
    "@types/node": "^14.11.8",
    "@typescript-eslint/eslint-plugin": "^4.5.0",
    "@typescript-eslint/parser": "^4.5.0",
    "eslint": "^7.11.0",
    "ts-node": "^9.0.0",
    "ts-node-dev": "^1.0.0",
    "typescript": "^4.0.3"
  }
}
